<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
<!-- For Object -->
	<bean id="createObjectDelegate" class="org.springframework.aop.framework.ProxyFactoryBean">
		<property name="proxyInterfaces">
			<value>
				com.delegate.CreateObjectDelegate
			</value>
		</property>
		<property name="target">
			<ref bean="createObjectDelegateImpl"/>
		</property>
		<property name="interceptorNames">
		      <list>
		        <value>loggingInterceptor</value>
		      </list>
   	 	</property>
	</bean>
	<bean id="createObjectDelegateImpl" class="com.delegate.impl.CreateObjectDelegateImpl">
		<property name="createObjectService">
			<ref bean="createObjectService"/>
		</property>
	</bean>

	 
	<bean id="createObjectService" class="org.springframework.aop.framework.ProxyFactoryBean">
		<property name="proxyInterfaces">
			<value>
				com.service.CreateObjectService
			</value>
		</property>
		<property name="target">
			<ref bean="createObjectServiceImpl"/>
		</property>
		<property name="interceptorNames">
		      <list>
		        <value>loggingInterceptor</value>
		      </list>
   	 	</property>
	</bean>
	<bean id="createObjectServiceImpl" class="com.service.impl.CreateObjectServiceImpl">
		<property name="createObjectDAO">
			<ref bean="createObjectDAO"/>
		</property>
		
		<property name="createTripleDAO">
			<ref bean="createTripleDAO"/>
		</property>
		
		<property name="predicateDAO">
			<ref bean="predicateDAO"/>
		</property>
	</bean>
	
	<bean id="createObjectDAO" class="org.springframework.aop.framework.ProxyFactoryBean">
		<property name="proxyInterfaces">
			<value>
				com.dao.CreateObjectDAO
			</value>
		</property>
		<property name="target">
			<ref bean="createObjectDAOImpl"/>
		</property>
		<property name="interceptorNames">
		      <list>
		        <value>loggingInterceptor</value>
		      </list>
   	 	</property>
	</bean>
	<bean id="createObjectDAOImpl" class="com.dao.impl.CreateObjectDAOImpl">
		<property name="sessionFactory">
			<ref bean="sessionFactory"/>
		</property>
	</bean>
	
	<bean id="createTripleDAO" class="org.springframework.aop.framework.ProxyFactoryBean">
		<property name="proxyInterfaces">
			<value>
				com.dao.CreateTripleDAO
			</value>
		</property>
		<property name="target">
			<ref bean="createTripleDAOImpl"/>
		</property>
		<property name="interceptorNames">
		      <list>
		        <value>loggingInterceptor</value>
		      </list>
   	 	</property>
	</bean>
	<bean id="createTripleDAOImpl" class="com.dao.impl.CreateTripleDAOImpl">
		<property name="sessionFactory">
			<ref bean="sessionFactory"/>
		</property>
	</bean>
	<bean id="predicateDAO" class="org.springframework.aop.framework.ProxyFactoryBean">
		<property name="proxyInterfaces">
			<value>
				com.dao.PredicateDAO
			</value>
		</property>
		<property name="target">
			<ref bean="predicateDAOImpl"/>
		</property>
		<property name="interceptorNames">
		      <list>
		        <value>loggingInterceptor</value>
		      </list>
   	 	</property>
	</bean>
	<bean id="predicateDAOImpl" class="com.dao.impl.PredicateDAOImpl">
		<property name="sessionFactory">
			<ref bean="sessionFactory"/>
		</property>
	</bean>
	<!-- For Object -->
</beans>