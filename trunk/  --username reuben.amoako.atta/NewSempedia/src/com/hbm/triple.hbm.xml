<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.hibernate.pojo.Triple" table="triple">
		<id name="tripleId" column="TRIPLE_ID" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		
		<many-to-one name="predicate" class="com.hibernate.pojo.Predicate"
		 lazy="false" column="PREDICATE_ID">
		</many-to-one>
		
		<!--<many-to-one name="subject" class="com.hibernate.pojo.Object"
		 lazy="false" column="OBJECT_ID" >
		</many-to-one>-->
		
		<!-- <many-to-one name="object" class="com.hibernate.pojo.Object"
		 lazy="false" column="OBJECT_ID">
		</many-to-one> -->
		
		<many-to-one name="subject" class="com.hibernate.pojo.ObjectPojo" 
			column="SUBJECT" foreign-key="object_id" lazy="false"/>
			
        <many-to-one name="object" class="com.hibernate.pojo.ObjectPojo" 
			column="OBJECT" foreign-key="object_id" lazy="false"/>
		
	</class>
</hibernate-mapping>
